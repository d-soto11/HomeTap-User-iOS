  pod 'FBSDKShareKit'
  pod 'JModalController'
  pod 'GoogleSignIn'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'ImagePicker'
  pod 'Lightbox'
  pod 'RestEssentials'
  pod 'DropDown'
  pod 'Stripe'
  pod 'ReachabilitySwift'
  pod 'Cosmos'
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'NewRelicAgent'
end

